                            <TextBlock x:Name="stdntdTXTBLCKNM" TextWrapping="Wrap" Margin="0,5,0,5" FontSize="12" FontFamily="arial">[value]</TextBlock>
                        </StackPanel>
                        <Button x:Name="gnrtpdfBTTNNM" Click="gnrtpdfBTTNNM_Click" Height="50" BorderThickness="0" Background="Transparent" Width="50" Grid.Column="1">
                            <Image Height="50" Source="Front End\exportpdfIC.png" Width="100"></Image>
                        </Button>
                    </Grid>
                    <TextBlock x:Name="crsTXTBLCKNM" TextWrapping="Wrap" Margin="0,5,0,5" FontSize="12" FontFamily="arial">[value]</TextBlock>
                    <TextBlock x:Name="yrrgrdTXTBLCKNM" TextWrapping="Wrap" Margin="0,5,0,5" FontSize="12" FontFamily="arial">[value]</TextBlock>
